binData <- function(data, low_Name = 'l', high_Name = 'u',  numBinned = 5){
vals <- data[[lName]]
sortedVals <- sort(vals, decreasing = TRUE)
bin_min = sortedVals[1]
bin_max = sortedVals[numBinned]
changeLow <- data[[low_Name]] > bin_min
changeHi  <- changeLow & data[[high_Name]] < bin_max
data[[low_Name]][changeLow] <- bin_min
data[[high_Name]][changeHi] <- bin_max
return(data)
}
